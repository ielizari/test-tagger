{
  "rootDir": "test",
  "include": [
    "**/*spec.js"
  ],
  "exclude": [
    "**/node_modules/**",
    "**/.git/**"
  ],
  "outputDir": "report",
  "autotag": {
    "peggy": {
      "disabled": true,
      "match": ["peggyjs", "peg", "pegjs"]
    },
    "regex": {
      "disabled": true,
      "match": ["regular expression", "regexp"]
    },
    "task": {
      "match": ["tarea"]
    },
    "filter": {
      "match": ["filtro", "filtra"]
    },
    "worklog": {
      "match": ["parte", "trabajo"]
    }
  }
}