{
  "rootDir": "test",
  "include": [
    "**/*.spec.js",
    "**/*.test.js"
  ],
  "exclude": [
    "**/node_modules/**",
    "**/.git/**"
  ],
  "outputDir": "report",
  "autotags": [
    {
      "tag": "peggy",
      "disabled": false,
      "match": ["peggyjs", "peg", "pegjs"]
    },
    {
      "tag": "autotag",
      "match": ["autotag", "automatic", "autotags", "auto-tag"]
    },
    {
      "tag": "regex",
      "disabled": false,
      "match": ["regular expression", "regexp"]
    }
  ]
}